class Solution:
    def isPalindrome(self, s: str) -> bool:
        l = ''
        for i in s:
            if i.isalnum():
                l += i
        if l == l[::-1]:
            return True
        else:
            return False


class Solution:
    def isPalindrome(self, x: int) -> bool:
        s = str(x)
        if s == s[::-1]:
            return True
        else:
            return False
